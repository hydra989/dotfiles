# bookkeeper
~/s/dotfiles/.config/bookkeeper/config<->~/.config/bookkeeper/config

# emacs
~/s/dotfiles/.emacs.d/src->~/.emacs.d/src
~/s/dotfiles/.emacs.d/init.el<->~/.emacs.d/init.el

# rofi
~/s/dotfiles/.config/rofi/config.rasi<->~/.config/rofi/config.rasi

# kitty
~/s/dotfiles/.config/kitty/kitty.conf<->~/.config/kitty/kitty.conf

# bash
~/s/dotfiles/.bashrc<->~/.bashrc

# vim
~/s/dotfiles/.vimrc<->~/.vimrc


# canary specifics
! HOSTNAME:canary
~/s/dotfiles/nix/configuration.nix<->/etc/nixos/configuration.nix
! HOSTNAME:canary
~/s/dotfiles/nix/machines/canary.nix<->/etc/nixos/active-machine.nix


